{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dfdemo1920"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"linkedsource_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'linkedsource'"
		},
		"sinklinked_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'sinklinked'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storbrowsejobs.dfs.core.windows.net/"
		},
		"linkedsource_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storbrowsejobs.dfs.core.windows.net/"
		},
		"sinklinked_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "demobrowsejobsserver.database.windows.net"
		},
		"sinklinked_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "db1920"
		},
		"sinklinked_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "admin1920"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dfdemo1920_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"linkedsource_accountKey": {
						"value": "[parameters('linkedsource_accountKey')]"
					},
					"sinklinked_password": {
						"value": "[parameters('sinklinked_password')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"linkedsource_properties_typeProperties_url": {
						"value": "[parameters('linkedsource_properties_typeProperties_url')]"
					},
					"sinklinked_properties_typeProperties_server": {
						"value": "[parameters('sinklinked_properties_typeProperties_server')]"
					},
					"sinklinked_properties_typeProperties_database": {
						"value": "[parameters('sinklinked_properties_typeProperties_database')]"
					},
					"sinklinked_properties_typeProperties_userName": {
						"value": "[parameters('sinklinked_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "dfdemo1920_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'dfdemo1920_ArmTemplate_0')]"
			]
		}
	]
}